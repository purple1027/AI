package javaBasic02.ch05.sec01;

import java.util.Scanner;

public class 참조타입 {

	public static void main(String[] args) {
	
		int n=10; //스택공간에 올라감
	 Scanner sc= new Scanner(System.in); // new 힙 메모리에 scanner라는 클래스를 올려주세요 라는 의미
		                    // 뉴는 힙에 올라감
	
	// 힙 메모리에 주소를 스택 공간의 변수에 저장해야 한다
	
    System.out.println(n);

	//변수형식은 자료형 변수명;
    
    student st=new student();
    
    
	
	
	
	} //main
 
} //class
