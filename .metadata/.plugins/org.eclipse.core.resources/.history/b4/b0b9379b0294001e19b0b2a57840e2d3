package javaBasic02.ch05.sec03;

public class 배열연습 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		//배열: 동일한 자료들의 집합 (정수형 배열, 문자형 배열 등등) , 연속해서 저장되어 있음
		//배열 쓰는 이유 ? 정수 100개를 기억해야 한다면 변수 100개를 만들어야 하는것이 불편함
		//배열 만드는 방법은? 1차원 2차원 3차원
		
		int[] number=new int[] {1,2,3,4,5};
		String[] ABC=new String[] {"a","b"};
		double[] numbers=new double[] {1.1, 2.2, 3.3};
		boolean[] ox=new boolean[] {true, false};
	    byte[]count=new byte[] {1,2,3};
		short[] number2=new short[] {6,7};
		float[] number3=new float[] {1.1f, 2.2f};
		
		
		//arr 배열의 [0]의 값 1을 출력하려면
	 System.out.println(number[0]);
	 System.out.println(ABC[0]);
	 System.out.println(numbers[2]);
		
		
		
		
		
		
		
		
		
		
	} //main

} //class
