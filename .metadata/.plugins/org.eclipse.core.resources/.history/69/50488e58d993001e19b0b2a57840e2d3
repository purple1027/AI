
public class while문연습 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// 2 4 6 8 10 출력해보기
		// 2를 정수형 변수에 넣는데
	    // 정수형 변수를 2씩 더한다
		// 반복해서 출력한다.
		// 10이 넘으면 끝난다
		
		
		int number = 2;

		
		
		while(number < 10) {

		System.out.println(number);
		number = number + 2;
				
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
