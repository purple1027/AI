package javaBasic03.ch06.sec9;

public class Account {

	String 계좌번호;
	String 계좌주;
	int 초기입금액;
	int 잔고;
	
	public Account() {
		super();
	}

	public Account(String 계좌번호, String 계좌주, int 초기입금액, int 잔고) {
		super();
		this.계좌번호 = 계좌번호;
		this.계좌주 = 계좌주;
		this.초기입금액 = 초기입금액;
		this.잔고 = 잔고;
	}
	

	
	
}
