package javaBasic03.ch06.sec2;

public class 정적클래스 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//정적이라는 말은 Static이 붙어있는 변수, 메소드
		
     
		m1();
		
		
		
		
		
	}// 메인
	
	static void m1() {
		
		System.out.println("정적 메소드입니다.");
	}
	
    static void m2() {
		
		System.out.println("인스턴스 메소드입니다.");
	}

} //클래스
